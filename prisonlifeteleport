if game.PlaceId ~= 155615604 then 
    return
end

while not game:IsLoaded() or not game:GetService("CoreGui") or not game:GetService("Players").LocalPlayer or
    not game:GetService("Players").LocalPlayer.PlayerGui do
    wait()
end
 game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Click F1 for cops base and F2 to return";
        Text = "";
        }) 
    
local function stupidgame()
local input = game:GetService("UserInputService")
    input.InputBegan:connect(function(inst)
    if inst.KeyCode == Enum.KeyCode.F1 then
 wait(0.2)
 
     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(854,100,2259) -- police area
    end
    end)
    end

coroutine.wrap(stupidgame)()
    local SavePosition = Instance.new("TextLabel")
local input = game:GetService("UserInputService")
    input.InputBegan:connect(function(inst)
    if inst.KeyCode == Enum.KeyCode.F1 then
 task.wait()
                lastlocationx = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.x
                lastlocationy = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.y
                lastlocationz = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.z
                SavePosition.Text = "Last Place: " ..lastlocationx.. "," ..lastlocationy.. "," ..lastlocationz -- where you can save your cords too

                
                input.InputBegan:connect(function(inst)
    if inst.KeyCode == Enum.KeyCode.F2 then
 task.wait()
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(lastlocationx, lastlocationy, lastlocationz)
 
    end
                end)
end
end)
